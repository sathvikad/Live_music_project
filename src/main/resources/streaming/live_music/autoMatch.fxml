<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.AutoMatchController" spacing="15" alignment="CENTER" style="-fx-padding: 20;">

    <Label text="Auto Match Venues" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

    <TableView fx:id="matchedRequestTable" prefHeight="300">
        <columns>
            <TableColumn fx:id="eventNameColumn" text="Event Name" prefWidth="150"/>
            <TableColumn fx:id="venueNameColumn" text="Venue Name" prefWidth="150"/>
            <TableColumn fx:id="statusColumn" text="Status" prefWidth="100"/>
        </columns>
    </TableView>

    <Button text="Back to Dashboard" onAction="#handleBack"/>
</VBox>
