<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.AutoMatchController" alignment="CENTER" spacing="10" prefWidth="600" prefHeight="300">

    <Label text="Auto Match Venues" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
