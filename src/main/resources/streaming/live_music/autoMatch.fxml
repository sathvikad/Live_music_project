<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.AutoMatchController" style="-fx-padding: 20;">

    <Label text="Auto Match System" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Button text="Run Auto Match" fx:id="matchButton" onAction="#handleAutoMatch"/>
    <Label fx:id="resultLabel" text="Matching Results will appear here" wrapText="true"/>

    <Button text="Back to Dashboard" onAction="#handleBackToDashboard"/>
</VBox>
