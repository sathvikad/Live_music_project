<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="Auto-Match Venues" style="-fx-font-size: 18px;" BorderPane.alignment="center"/>
    </top>
    <center>
        <TableView fx:id="matchTable">
            <columns>
                <TableColumn fx:id="requestIDColumn" text="Request ID" />
                <TableColumn fx:id="venueNameColumn" text="Venue Name" />
                <TableColumn fx:id="matchScoreColumn" text="Match Score" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button text="Back to Dashboard" onAction="#handleBackToDashboard" />
    </bottom>
</BorderPane>
