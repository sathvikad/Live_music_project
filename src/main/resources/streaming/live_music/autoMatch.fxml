<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.AutoMatchController">

    <Label text="Auto Match Venues" style="-fx-font-size: 20px; -fx-font-weight: bold;" />

    <TextArea fx:id="matchResultsArea" prefWidth="500" prefHeight="300" wrapText="true" />

    <!-- Fixed HBox reference -->
    <HBox spacing="15">
        <Button text="Start Matching" onAction="#handleStartMatching" />
        <Button text="Back to Dashboard" onAction="#handleBack" />
    </HBox>
</VBox>
