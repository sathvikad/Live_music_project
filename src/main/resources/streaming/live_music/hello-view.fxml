<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="15">
    <Label text="Live Music Venue Matchmaker" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

    <Button text="View Job Requests" onAction="#handleJobRequests" prefWidth="200" />
