<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.IncomeChartController" spacing="10" style="-fx-padding: 20;">

    <Label text="Income vs Commission per Order" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

    <BarChart fx:id="incomeChart">
        <xAxis>
            <CategoryAxis label="Event Title" />
        </xAxis>
        <yAxis>
            <NumberAxis label="Amount ($)" />
        </yAxis>
    </BarChart>
</VBox>