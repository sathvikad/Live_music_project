<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="Job Requests" style="-fx-font-size: 18px;" BorderPane.alignment="center"/>
    </top>
    <center>
        <TableView fx:id="jobRequestTable">
            <columns>
                <TableColumn fx:id="idColumn" text="Request ID" />
                <TableColumn fx:id="clientColumn" text="Client" />
                <TableColumn fx:id="eventTypeColumn" text="Event Type" />
                <TableColumn fx:id="dateColumn" text="Event Date" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button text="Back to Dashboard" onAction="#handleBackToDashboard" />
    </bottom>
</BorderPane>
