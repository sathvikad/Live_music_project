<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" spacing="10" alignment="CENTER">
    <Label text="Job Requests" style="-fx-font-size: 20px; -fx-font-weight: bold;" />

    <TableView fx:id="jobTable" prefWidth="600">
        <columns>
            <TableColumn text="Request ID" prefWidth="150" />
            <TableColumn text="Event Name" prefWidth="250" />
            <TableColumn text="Client" prefWidth="200" />
        </columns>
    </TableView>

    <Button text="Back to Menu" onAction="#handleBack" />
</VBox>
