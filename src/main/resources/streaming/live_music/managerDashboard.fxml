<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="15" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.ManagerDashboardController" style="-fx-padding: 20;">

    <Label text="Manager Dashboard" style="-fx-font-size: 20px; -fx-font-weight: bold;"/>

    <Button text="View Venues" onAction="#handleViewVenues"/>
    <Button text="Add New Job Request" onAction="#handleAddJobRequest"/>
    <Button text="View Job Requests" onAction="#handleJobRequests"/>
    <Button text="Auto Match Venues" onAction="#handleAutoMatch"/>

    <!-- Added Buttons -->
    <Button text="View Bookings" onAction="#handleViewBookings"/>
    <Button text="View Pie Chart" onAction="#handleViewPieChart"/>

    <Button text="Logout" onAction="#handleLogout"/>

</VBox>
