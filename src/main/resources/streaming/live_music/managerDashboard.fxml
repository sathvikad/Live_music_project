<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="15" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="streaming.live_music.ManagerDashboardController" style="-fx-padding: 20;">

    <Label text="Manager Dashboard" style="-fx-font-size: 20px; -fx-font-weight: bold;" />

    <Button text="View Venues" onAction="#handleViewVenues" />
    <Button text="Add Venue" onAction="#handleAddVenue" />
    <Button text="Job Requests" onAction="#handleJobRequests" />
    <Button text="Logout" onAction="#handleLogout" />
</VBox>
