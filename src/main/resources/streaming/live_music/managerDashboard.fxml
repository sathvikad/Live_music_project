<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefWidth="600" prefHeight="400" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="streaming.live_music.ManagerDashboardController">

    <Text text="Manager Dashboard" layoutX="220" layoutY="50" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Button text="View Venues" layoutX="200" layoutY="100" prefWidth="200" fx:id="viewVenuesButton" onAction="#handleViewVenues"/>
    <Button text="Job Requests" layoutX="200" layoutY="150" prefWidth="200" fx:id="jobRequestsButton" onAction="#handleJobRequests"/>
    <Button text="Add Venue" layoutX="200" layoutY="200" prefWidth="200" fx:id="addVenueButton" onAction="#handleAddVenue"/>
    <Button text="Venue Utilization" layoutX="200" layoutY="250" prefWidth="200" fx:id="venueUtilizationButton" onAction="#handleVenueUtilization"/>

</AnchorPane>
