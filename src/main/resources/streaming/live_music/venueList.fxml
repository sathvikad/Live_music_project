<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="10">
    <Label text="Venue List" style="-fx-font-size: 20px; -fx-font-weight: bold;" />

    <TableView fx:id="venueTable" prefWidth="600">
        <columns>
            <TableColumn text="Venue Name" fx:id="colVenueName" prefWidth="200" />
            <TableColumn text="Capacity" fx:id="colVenueCapacity" prefWidth="200" />
            <TableColumn text="Location" fx:id="colVenueLocation" prefWidth="200" />
        </columns>
    </TableView>

    <Button text="Back to Menu" onAction="#handleBack" />
</VBox>
