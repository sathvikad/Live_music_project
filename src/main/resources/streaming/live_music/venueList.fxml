<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefWidth="800" prefHeight="500" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="streaming.live_music.VenueListController">

    <TableView fx:id="venueTable" layoutX="50" layoutY="50" prefWidth="700" prefHeight="350">
        <columns>
            <TableColumn text="Venue Name" fx:id="nameColumn" prefWidth="200"/>
            <TableColumn text="Capacity" fx:id="capacityColumn" prefWidth="100"/>
            <TableColumn text="Suitable For" fx:id="suitableForColumn" prefWidth="150"/>
            <TableColumn text="Category" fx:id="categoryColumn" prefWidth="150"/>
            <TableColumn text="Booking Price" fx:id="bookingPriceColumn" prefWidth="120"/>
        </columns>
    </TableView>

    <Button text="Back to Dashboard" layoutX="50" layoutY="420" fx:id="backButton" onAction="#handleBackToDashboard"/>
</AnchorPane>
