<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="Available Venues" style="-fx-font-size: 18px;" BorderPane.alignment="center"/>
    </top>
    <center>
        <TableView fx:id="venueTable">
            <columns>
                <TableColumn fx:id="venueNameColumn" text="Venue Name" />
                <TableColumn fx:id="capacityColumn" text="Capacity" />
                <TableColumn fx:id="locationColumn" text="Location" />
                <TableColumn fx:id="availabilityColumn" text="Availability" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button text="Back to Dashboard" onAction="#handleBackToDashboard" />
    </bottom>
</BorderPane>
